<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodmart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodMart">
        <activity android:name=".CartItemList"></activity>
        <activity android:name=".DeliveryAddedItemList" />
        <activity android:name=".AddDriver" />
        <activity android:name=".AddProduct" />
        <activity android:name=".DeliveryList" />
        <activity android:name=".ManageEmployeee" />
        <activity android:name=".ManageProduct" />
        <activity android:name=".AdminHome" />
        <activity android:name=".CustomerHome" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".ChooseActor" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>